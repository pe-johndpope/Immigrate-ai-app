{
    "compilerOptions": {
      "allowJs": true,
      "allowSyntheticDefaultImports": true,
      "experimentalDecorators": true,
      "jsx": "react-native",
      "module": "es2015",
      "moduleResolution": "node",
      "noImplicitAny": false,
      "noImplicitReturns": true,
      "noImplicitThis": true,
      "noUnusedLocals": true,
      "sourceMap": true,
      "target": "esnext",
      "lib": [
        "esnext",
        "dom"
      ],
      "skipLibCheck": true,
      "resolveJsonModule": true
    },
    "exclude": [
      "node_modules"
    ],
    "include": [
      "App.tsx",
    ],
    "extends": "expo/tsconfig.base"
    
  }
  

  